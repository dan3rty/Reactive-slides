{
  "extends": [
    "stylelint-config-standard"
  ],
  "plugins": [
    "stylelint-order",
    "stylelint-prettier"
  ],
  "rules": {
    "order/properties-order": [
      "width",
      "height"
    ],
    "color-hex-length": "long",
    "color-no-invalid-hex": true,
    "prettier/prettier": true,
    "selector-class-pattern": null,
    "keyframes-name-pattern": null,
    "block-no-empty": null
  }
}